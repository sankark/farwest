<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<link rel="stylesheet" href="/static/css/960.css" />
<title>Insert title here</title>
</head>
<body>
	<div class="container_12">
	<form id="form" action="" method="put">
		<div class="grid_12" >
			<div class="grid_3 suffix_1">UserName</div>
			<div class="grid_3"><input name="user_name" value="{{user_data.user_name}}"></input></div>
		</div>
		<div class="grid_12">
			<div class="grid_3 suffix_1">Password</div>
			<div class="grid_3"><input name="password">{{user_data.password}}</input></div>
		</div>
		
		<div class="grid_12">
			<div class="grid_3 suffix_1">Email-Id</div>
			<div class="grid_3"><input name="email_id">{{user_data.email_id}}</input></div>
		</div>
		
		<div class="grid_12">
			<div class="grid_3 suffix_1">Address</div>
			<div class="grid_3"><input name="address">{{user_data.address}}</input></div>
		</div>
		
		<div class="grid_12">
			<div class="grid_3 suffix_1">Phone</div>
			<div class="grid_3"><input name="phone_no">{{user_data.phone_no}}</input></div>
		</div>
		
		<div class="grid_12">
			<div class="grid_3 prefix_1"><input type="submit" class="btn" value="sign-up"></div>
		</div>
	</form>
	</div>
	
	<script src="/static/bootstrap/js/bootstrap.min.js"></script>
	<script src="/static/jquery/jquery.min.js"></script>
	<script src="/static/jquery/jquery.form.js"></script>
	<script src="/static/jquery/jquery.json.js"></script>
	
<script src="/static/js/form_submit.js"></script>
</body>
</html>